{
    "version": 2,
    "builds": [
        {
        "src": "index.js",
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "index.js"
        }
    ],
    "rewrites": [
        { "source": "/css/(.*)", "destination": "/public/css/$1" },
        { "source": "/js/(.*)", "destination": "/public/js/$1" }
      ]
}